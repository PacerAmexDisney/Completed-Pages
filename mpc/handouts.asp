<h3 class="pubcat" style="margin-bottom: .5em;"><a name="english">English</a></h3>

		<%
		sql = "SELECT * FROM Publications WHERE pub_mpc = 'yes' AND (pub_spanish = '' OR pub_spanish IS NULL) AND (pub_somali = '' or pub_somali IS NULL) AND (pub_hmong = '' OR pub_hmong IS NULL) AND pub_type = 'handout' ORDER BY pub_name"
